package com.kosta.sample1;

public class Car {
	String model;
	int price;
	
	// DI(Dependency Injection)
	public Car(String model, int price) {
		super();
		this.model = model;
		this.price = price;
	}

	public void carPrint() {
		System.out.println("car의 정보입니다.");
		System.out.println("차의 모델: " + model);
		System.out.println("차의 가격: " + price);
	}
}
