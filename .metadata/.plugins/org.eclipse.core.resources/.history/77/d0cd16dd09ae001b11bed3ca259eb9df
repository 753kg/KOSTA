package com.kosta.sample1;

public class TVUser {

	public static void main(String[] args) {
		test2();
	}
	
	private static void test2() {
		// 변경사항이 생겼을 때 모든 걸 바꿔야한다.
		LgTV tv = new LgTV();
		tv.turnOn();
		tv.turnOff();
	}

	private static void test1() {
		SamsungTV tv = new SamsungTV();
		tv.powerOn();
		tv.powerOff();
	}

}
