package com.kosta.sample1;

public class TVUser {

	public static void main(String[] args) {
		test2();
	}
	
	private static void test2() {
		// 변경사항이 생겼을 때 수정을 최소한으로 하기 위해서 인터페이스 사용
		// 사용자가 new --> 생성자가 바뀔 경우 ,,,
		TV tv = new SamsungTV();
		tv.powerOn();
		tv.powerOff();
	}

	private static void test1() {
		SamsungTV tv = new SamsungTV();
		tv.powerOn();
		tv.powerOff();
	}

}
